{
  "name": "hack-2019-electron",
  "description": "Create 'karaoke-style' videos of Bible passages to help users become comfortable at reading the Bible in another language.",
  "author": {
    "name": "DigiServe",
    "email": "info@digiserve.org",
    "url": "https://digiserve.org/"
  },
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "electron": "^6.0.11",
    "electron-builder": "^22.1.0",
    "foreman": "^3.0.1"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@blueprintjs/core": "^3.18.1",
    "@blueprintjs/select": "^3.10.0",
    "bbk": "github:Hiro-Nakamura/bible-karaoke-cli",
    "customize-cra": "^0.8.0",
    "datauri": "^2.0.0",
    "ffbinaries": "^1.1.4",
    "fluent-ffmpeg": "^2.1.2",
    "font-list": "^1.2.2",
    "mobx": "^5.14.0",
    "mobx-react": "^6.1.3",
    "node-sass": "^4.13.1",
    "puppeteer": "^1.20.0",
    "react": "^16.10.2",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0",
    "shelljs": "^0.8.3",
    "tempy": "^0.3.0",
    "vtt-json": "^1.0.1"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "electron": "electron --inspect=5858 .",
    "dev": "nf start -p 3000",
    "prepack": "npm run build",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "productName": "Bible Karaoke",
    "appId": "bible.karaoke",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "files": [
      "build/*",
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
